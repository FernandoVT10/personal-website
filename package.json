{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./server/index.ts --exec 'ts-node --project \"tsconfig.server.json\"'",
    "build:next": "next build",
    "build:server": "tsc -p 'tsconfig.server.json'",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node ./build"
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "apollo-server-express": "^2.22.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "next": "10.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
